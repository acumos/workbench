{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../out-tsc/app",
    "module": "es2015",
    "baseUrl": "./",
    "paths": {
      "@angular/*": [
        "../node_modules/@angular/*"
      ],
      "@nebular/*": [
        "../node_modules/@nebular/*"
      ]
    },
	  "skipLibCheck" : true,
  },
  "exclude": [
    "test.ts",
    "**/*.spec.ts",
    "../node_modules/*"
  ],
  "include": [
    "../src/*.ts",
    "../src/**/*.ts"
  ]
}
