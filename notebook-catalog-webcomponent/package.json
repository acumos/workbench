    "build:component": "cd vue-component && npm run build:webcomponent",
    "postinstall": "run-s install:*",
    "start:dev": "run-p run:*"
  },
  "devDependencies": {},
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
